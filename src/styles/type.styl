body, input
  font-family: 'Overpass', sans-serif
  color: alpha(darken($colorPrimary, 70%), .7)
  font-weight:500

.alt
  font-family: 'Raleway', sans-serif

h1,h2,h3,h4,h5,h6,p
  display:block
  width:100%

h1
  font-size: 3em
  font-weight: 700
  margin-top: 2em
  line-height: 1.2em
h2
  font-size: 2.6em
  font-weight: 500
  margin-top: 1.5em
  line-height: 1.2em
h3
  font-size: 2.2em
  font-weight: 500
  margin-top: 1.17em
  line-height: 1.2em
h4
  font-size: 1.8em
  font-weight: 500
  margin-top: 1.12em
  line-height: 1.2em
h5
  font-size: 1.4em
  font-weight: 600
  margin-top: .83em
  line-height: 1.2em
h6
  font-size: 1em
  font-weight: 700
  margin-top: .75em
  line-height: 1.2em

p
  line-height: 1.2em
  margin-top: .5em

strong
  font-weight:600

em
  font-style: italic

small
  font-size: .7em

.strike
  text-decoration: line-through

.underline
  text-decoration: underline

